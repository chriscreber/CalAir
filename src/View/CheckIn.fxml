<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane layoutX="0.0" layoutY="0.0" prefHeight="375.0" prefWidth="690.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.CheckInController">
    <children>
        <Label layoutX="271.0" layoutY="33.0" text="Check In">
            <font>
                <Font size="36.0" />
            </font>
        </Label>
        <TextField fx:id="confirmText" layoutX="206.0" layoutY="113.0" prefHeight="27.0" prefWidth="279.0" promptText="Enter Confirmation Number" />
        <Label fx:id="checkInReturn" alignment="CENTER" layoutX="196.0" layoutY="287.0" prefHeight="22.0" prefWidth="300.0" textFill="#2fda0b">
            <font>
                <Font name="System Bold" size="17.0" />
            </font>
        </Label>
        <Button layoutX="315.0" layoutY="227.0" mnemonicParsing="false" onAction="#submitCheckIn" text="Submit" />
        <TextField fx:id="bagsText" layoutX="206.0" layoutY="151.0" prefHeight="27.0" prefWidth="279.0" promptText="Enter Number of Bags" />
        <Hyperlink layoutX="284.0" layoutY="178.0" onAction="#openLink" text="Click for Baggage Policy">
            <font>
                <Font size="10.0" />
            </font>
        </Hyperlink>
    </children>
</AnchorPane>
